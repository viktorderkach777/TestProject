pipeline {


agent any

environment {

dotnet = '/usr/share/dotnet/dotnet'

}

stages {

stage('Checkout') {

steps {

checkout scm

}

}

stage('Restore Packages') {

steps {

dir("awesome-project") {

sh "ls -la"

sh "pwd"

sh 'dotnet --info'

sh 'dotnet restore'

}

}

}

stage('DotNet Test') {

steps {

dir("awesome-project") {

sh "ls -la"

sh "pwd"

sh 'dotnet --info'

  echo "${TestCategory}"

}

}

}





}

}









