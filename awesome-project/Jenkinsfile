pipeline {


agent any 
  parameters {
        string(defaultValue: 'Login', name: 'TestCategory', description: 'Enter the testcategory')
    }

environment {

dotnet = '/usr/share/dotnet/dotnet'

}

stages {

stage('Checkout') {

steps {

checkout scm

}

}

stage('Restore Packages') {

steps {

dir("awesome-project") {

sh "ls -la"

sh "pwd"

sh 'dotnet --info'

sh 'dotnet restore'

}

}

}

stage('DotNet Test') {

steps {

dir("awesome-project") {

sh "ls -la"

sh "pwd"

sh 'dotnet --info'

  echo "${TestCategory}"

}

}

}





}

}









