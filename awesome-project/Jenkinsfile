node {
    stage 'Checkout'
		checkout scm
    
    stage('Example') {
        if (env.BRANCH_NAME == 'master') {
            echo 'I only execute on the master branch'
            sh 'ls -la'
        } else {
            echo 'I execute elsewhere'
             sh 'ls -la'
             dir("awesome-project") {
               sh "ls -la"
               sh "pwd"
               sh 'dotnet --info'
               sh 'dotnet restore'
             }

        }
    }
}
